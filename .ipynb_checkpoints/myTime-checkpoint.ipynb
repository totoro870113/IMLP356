    "import time\n",
    "#%Y-%m-%d-%H:%M:%S\n",
    "#help(time.strftime)\n",
    "\n",
    "def GetTime(format):\n",
    "    return time.strftime(format,time.localtime())\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    print(GetTime(\"%Y-%m-%d\"))\n",
    "    print(GetTime(\"%Y-%m-%d-%H:%M:%S\"))\n",
    "    print(GetTime(\"%Y-%m-%d-%H:%M:%S %a %A\"))\n",
    "    \n",
    "    \n",
    "\n"
   ]
